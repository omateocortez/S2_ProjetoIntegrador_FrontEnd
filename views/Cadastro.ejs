<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="cadastro.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded" rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;1,200;1,300;1,400;1,500;1,600&display=swap" rel="stylesheet">
    <link rel="icon" type="image/x-icon" href="imgs/MnaMBranco.png">
    <title>Home - Mãos Na Massa</title>
</head>
<body>
<!------------------------ Header ------------------------->
<%- include('partials/Header') %>    
<!------------------------ Header ------------------------->

    <section id="login">
        <div class="container mx-auto my-5">
            <div class="row">
                <div class="col-11 col-md-10 col-lg-8 mx-auto glass">
                    <h1 class="text-center mb-4 cadastro-membro">Cadastro de Voluntário</h1>
                    <form class="d-flex justify-content-center">
                        <div class="col-5 mb-2 nome-form">
                            <label for="nome1" class="form-label">Nome</label>
                            <input type="text" id="nome1" class="form-control p-3 fs-5" placeholder="Ex.: João">
                        </div>
                        <div class="col-5 mb-2">
                            <label for="nome2" class="form-label">Sobrenome</label>
                            <input type="text" id="nome2" class="form-control p-3 fs-5" placeholder="Ex.: Silva">
                        </div>
                    </form>
                    
                    <form>
                        <div class="mx-5 mb-2">
                            <label for="email" class="form-label fs-4">Endereço de E-mail</label>
                            <input type="email" id="email" class="form-control p-3 fs-5" placeholder="Digite seu e-mail">
                        </div>
                        
                        <div class="mx-5 my-4">
                            <label for="Senha" class="form-label fs-4">Senha</label>
                            <div class="input-group input-group-background">
                                <input type="password" id="Senha" class="form-control p-3 fs-5" placeholder="Digite sua senha">
                                <button class="btn btn-visibility" type="button" id="togglePassword">
                                    <span class="material-symbols-rounded visibility-icon">visibility_off</span>
                                </button>
                            </div>
                        </div>                        

                        <div class="row justify-content-center">
                            <div class="justify-content-center botoes">
                                <div class="col justify-content-center">
                                    <div type="button" class="text-center confirmar">Confirmar</div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>







<!------------------------ Footer --------------------------->
<%- include('partials/Footer') %>    
<!------------------------ Footer --------------------------->
 
 </body>
 </html>